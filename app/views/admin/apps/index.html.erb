<h1>Listing admin_apps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Access key</th>
      <th>Secret key</th>
      <th>Permission level</th>
      <th>Enable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_apps.each do |admin_app| %>
      <tr>
        <td><%= admin_app.name %></td>
        <td><%= admin_app.access_key %></td>
        <td><%= admin_app.secret_key %></td>
        <td><%= admin_app.permission_level %></td>
        <td><%= admin_app.enable %></td>
        <td><%= link_to 'Show', [:admin, admin_app] %></td>
        <td><%= link_to 'Edit', edit_admin_app_path([:admin, admin_app]) %></td>
        <td><%= link_to 'Destroy', [:admin, admin_app], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App', new_admin_app_path %>
