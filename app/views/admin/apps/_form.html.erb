<%= simple_form_for([:admin, @admin_app]) do |f| %>
    <%= f.error_notification %>

    <div class="form-inputs">
      <%= f.input :name %>
      <%= f.input :access_key %>
      <%= f.input :secret_key %>
      <%= f.input :permission_level %>
      <%= f.input :enable %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
<% end %>
